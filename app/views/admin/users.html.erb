<div class="box_centered">
	<h2>Usuarios R Con Erre</h2>
	<% @all_users.each do |u| %>
		<p><%= u.id %> - <%= u.email %> - Creado: <%= u.created_at.day %> de <%= u.created_at.month %> de <%= u.created_at.year %> | Último inicio de sesión: <%= u.last_sign_in_at.day %> de <%= u.last_sign_in_at.month %> <%= u.last_sign_in_at.year %> </p>
	<% end %>
</div>